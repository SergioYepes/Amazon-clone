                "href":"https://www.google.com/intl/es_co/drive",
                "noFollow":true,
                "tagTitle":"google drive title",
                "text":"google drive"
            }
        }
    },
    "menu-item#category2":{
        "props":{
            "id":"category2",
            "type":"custom",
            "itemProps":{
                "type":"external",
                "href":"https://fonts.google.com/knowledge",
                "noFollow":true,
                "tagTitle":"google fonts title",
                "text":"google fonts"
            }
        }
    },
    "menu-item#department2":{
        "props":{
            "id":"department2",
            "type":"custom",
            "itemProps":{
                "type":"external",
