{
    "menu#header__mobile":{
        "children":[
            "menu-item#department1",
            "menu-item#department2"
        ],
        "props":{
            "orientation":"vertical"
        }
    },
    "menu-item#department1":{
        "props":{
            "id":"department1",
            "type":"custom",
            "itemProps":{
                "type":"external",
                "href":"https://www.google.com",
                "noFollow":true,
                "tagTitle":"google title",
                "text":"Google"
            }
        },
        "blocks": [
            "submenu.accordion#department1"
        ]
    },
    "submenu.accordion#department1":{
        "children": [
            "menu#department1"
        ]
    },
    "menu#department1":{
        "children": [
            "menu-item#category1",
            "menu-item#category2"
        ]
    },
    "menu-item#category1":{
        "props":{
            "id":"category1",
            "type":"custom",
            "itemProps":{
